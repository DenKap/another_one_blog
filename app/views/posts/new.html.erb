<div class="post-form">
  <div class="wrapper">
    <div class="page-header"><h1>Add New Post</h1></div>
    <div class="add-form">
      <%=form_for @post, url: category_posts_path, html: { multipart: true } do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>