<div class="wrapper">
<div class="container">
  <div class="page-header">
    <h1>Edit <%=@post.name %></h1>
  </div>
  <div class="row">
    <div class="col-xs-12 .col-sm-6 .col-md-8">
      <%=form_for @post, url: category_post_path, html: { multipart: true } do |f| %>
        <%= render partial: 'form', locals: { f: f } %>
      <% end %>
    </div>
  </div>
</div>
</div>